        <name>Rollback to Previous Version</name>
        <onclick/>
        <order>101</order>
        <script><![CDATA[(function() {
	
	try {
		var inputs = {
			'demo': current.name,
			'version': current.previous_version
		};

		// Start Asynchronously: Uncomment to run in background. Code snippet will not have access to outputs.
		sn_fd.FlowAPI.getRunner().subflow('x_147145_demo_mark.dm_apply_changes_demo_app').inBackground().withInputs(inputs).run();
		
		gs.addInfoMessage('Rolling back ' + current.name + ' to previous version ' + current.previous_version + '. Please refer to the \'Status\' field in the \'Installation Status\' tab to get the current installation status.');
		action.setRedirectURL(current);
	} catch (ex) {
		var message = ex.getMessage();
		gs.error(message);
	}
	
})();]]></script>
